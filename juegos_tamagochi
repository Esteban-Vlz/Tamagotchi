    ..ccdddbbbdd55cbcdc.....
    ....ccbbcbddddccdddcc...
    ......cccdd555dcccccc...
    ........cccccccc........
    `, SpriteKind.Player)
vivorita = sprites.create(img`
    . . . c c c c c c . . . . . . . 
    . . c 6 7 7 7 7 6 c . . . . . . 
    . c 7 7 7 7 7 7 7 7 c . . . . . 
    c 6 7 7 7 7 7 7 7 7 6 c . . . . 
    c 7 c 6 6 6 6 c 7 7 7 c . . . . 
    f 7 6 f 6 6 f 6 7 7 7 f . . . . 
    f 7 7 7 7 7 7 7 7 7 7 f . . . . 
    . f 7 7 7 7 6 c 7 7 6 f . . . . 
    . . f c c c c 7 7 6 f c c c . . 
    . . c 6 2 7 7 7 f c c 7 7 7 c . 
    . c 6 7 7 2 7 7 c f 6 7 7 7 7 c 
    . c 1 1 1 1 7 6 6 c 6 6 6 c c c 
    . c 1 1 1 1 1 6 6 6 6 6 6 c . . 
    . c 6 1 1 1 1 1 6 6 6 6 6 c . . 
    . . c 6 1 1 1 1 1 7 6 6 c c . . 
    . . . c c c c c c c c c c . . . 
    `, SpriteKind.Enemy)
vidaDrogon = statusbars.create(20, 4, StatusBarKind.Health)
let vidaEnemigo = statusbars.create(20, 4, StatusBarKind.EnemyHealth)
vidaDrogon.attachToSprite(Drogon)
vidaEnemigo.attachToSprite(vivorita)
vidaDrogon.value = 20
Drogon.setPosition(13, 12)
scene.cameraFollowSprite(Drogon)
controller.moveSprite(Drogon)
tiles.setCurrentTilemap(tilemap`nivel1`)
tiles.placeOnRandomTile(vivorita, sprites.dungeon.doorLockedWest)
iniImage()
for (let value of tiles.getTilesByType(sprites.dungeon.floorLight0)) {
    PowerUps = sprites.create(Imaglista[randint(0, 3)], SpriteKind.Food)
    tiles.placeOnTile(PowerUps, value)
}
