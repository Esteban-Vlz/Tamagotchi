        . . 4 d f 2 2 2 2 2 2 f d 4 . . 
        . . 4 4 f 4 4 5 5 4 4 f 4 4 . . 
        . . . . . f f f f f f . . . . . 
        . . . . . f f . . f f . . . . . 
        `)) {
        sprites.destroy(otherSprite)
        vidaDrogon.value += 20
    }
})
let projectile: Sprite = null
let Imaglista: Image[] = []
let PowerUps: Sprite = null
let vidaDrogon: StatusBarSprite = null
let vivorita: Sprite = null
let Drogon: Sprite = null
let Atacar = 0
Drogon = sprites.create(img`
    ........................
    ........................
    ...........ccc..........
    ...........cccc.........
